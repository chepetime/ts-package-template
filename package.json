{
  "name": "@chepe/ts-package-template",
  "version": "0.0.2",
  "description": "A preconfigured project template for quickly starting a new TypeScript library",
  "license": "MIT",
  "author": {
    "name": "Jos√© Manuel Gulias Lugo Chepe",
    "email": "me@joselugo.dev",
    "url": "https://joselugo.dev"
  },
  "homepage": "https://github.com/chepetime/ts-package-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/chepetime/ts-package-template.git"
  },
  "bugs": "https://github.com/chepetime/ts-package-template/issues",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest watch --ui",
    "test:watch": "vitest watch"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@vitest/ui": "^0.33.0",
    "c8": "^8.0.0",
    "minifaker": "^1.34.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "keywords": [
    "package",
    "typescript"
  ],
  "access": "public",
  "packageManager": "pnpm@8.5.1",
  "typesVersions": {
    "*": {}
  }
}
